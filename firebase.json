{
  "hosting": {
    "site": "agendou",
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/id-**",
        "function": "agendamento"
      },
      {
        "source": "/",
        "destination": "/index.html"
      },
      {
        "source": "/index",
        "destination": "/index.html"
      },
      {
        "source": "/login",
        "destination": "/login.html"
      },
      {
        "source": "/admin",
        "destination": "/admin.html"
      },
      {
        "source": "/agendamento",
        "destination": "/agendamento.html"
      },
      {
        "source": "/politicas",
        "destination": "/politicas.html"
      },
      {
        "source": "/sitemap",
        "destination": "/sitemap.xml"
      },
      {
        "source": "/(css|js|json|images|img)/(.*)",
        "destination": "/$1/$2"
      },
      {
        "source": "/**",
        "destination": "/index.html"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  },
  "functions": {
    "source": "functions",
    "codebase": "default",
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log",
      "*.local"
    ]
  }
}